{
    "name": "fliq/unique-php",
    "description": "Package to interact with unique networks' Substrate rest client",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Pav",
            "email": "_@christians.email",
            "homepage": "https://christianpav.com"
        }
    ],
    "homepage": "https://github.com/fliq/unique-php",
    "keywords": ["UniqueNetwork", "Blockchain", "NFTs"],
    "require": {
        "guzzlehttp/guzzle": "^7.5",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-mock": "^1.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Fliq\\Unique\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fliq\\Unique\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
